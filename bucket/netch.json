    "pre_install": [
        "if (!(Test-Path \"$persist_dir\\mode_own\")) {",
        "    (New-Item -Force -ItemType \"directory\" -Path \"$dir\\mode_own\") | Out-Null",
        "}"
    ],
    "persist": [
        "data",
        "mode_own"
    ],
    "post_install": "(Copy-Item -Force -Path \"$dir\\mode_own\\*\" -Destination \"$dir\\mode\") | Out-Null",
    "shortcuts": [
        [
            "Netch.exe",
            "Netch"
        ]
    ],
    "checkver": "github",
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/NetchX/Netch/releases/download/$version/Netch.x64-$version.zip",
                "hash": {
                    "url": "https://github.com/NetchX/Netch/releases/tag/$version",
                    "mode": "extract",
                    "regex": "(?sm)x64.*?$sha256"
                }
            },
            "32bit": {
                "url": "https://github.com/NetchX/Netch/releases/download/$version/Netch.x86-$version.zip",
                "hash": {
                    "url": "https://github.com/NetchX/Netch/releases/tag/$version",
                    "mode": "extract",
                    "regex": "(?sm)x86.*?$sha256"
                }
            }
        }
    }
